{
  "extends": [],
  "linterOptions": {
    "exclude": []
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "no-unused-variable": true,
    "align": [
      true,
      "parameters",
      "arguments"
    ],
    "ban": [],
    "class-name": true,
    "curly": [
      true,
      "ignore-same-line"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      },
      {
        "call-signature": "onespace",
        "index-signature": "onespace",
        "parameter": "onespace",
        "property-declaration": "onespace",
        "variable-declaration": "onespace"
      }
    ],
    "interface-name": [
      true,
      "always-prefix"
    ],
    "max-classes-per-file": [
      true,
      1
    ],
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import |^} from"
      }
    ],
    "new-parens": true,
    "no-any": false,
    "no-consecutive-blank-lines": [
      true,
      1
    ],
    "no-eval": true,
    "no-var-keyword": true,
    "number-literal-format": true,
    "space-within-parens": [
      true,
      0
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "whitespace": [
      true,
      "check-decl"
    ]
  },
  "jsrules": {
    "align": [
      true,
      "parameters",
      "arguments"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import |^} from"
      }
    ],
    "new-parens": true,
    "no-consecutive-blank-lines": [
      true,
      1
    ],
    "no-eval": true
  }
}
